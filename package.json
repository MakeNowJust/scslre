{
  "name": "scslre",
  "version": "0.1.0",
  "description": "A library that finds JS RegExp with super-linear worst-case time complexity by searching for polynomial backtracking.",
  "main": "index",
  "scripts": {
    "check": "npm run lint && npx tsc --noEmit && cd tests && npx tsc --noEmit",
    "lint": "npx eslint --ignore-path .gitignore **/*.ts",
    "test": "cd tests && mocha -r ts-node/register '**/*.ts'",
    "build": "npx rimraf ./index.* .out/** && npx tsc && rollup -c && npm run build:dts && npm run build:docs",
    "build:dts": "dts-bundle --main ./.out/index.d.ts --name polynomial-regexp --out ../index.d.ts",
    "build:docs": "typedoc",
    "coverage": "npx nyc --reporter=html --reporter=text npm run test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "regex",
    "regexp",
    "backtracking",
    "polynomial",
    "exponential"
  ],
  "author": "Michael Schmidt",
  "homepage": "https://github.com/RunDevelopment/polynomial-regexp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/RunDevelopment/polynomial-regexp.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.31",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^7.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.31.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^8.8.1",
    "typedoc": "^0.20.0-beta.31",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "refa": "^0.6.0",
    "regexp": "^1.0.0"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ]
}
